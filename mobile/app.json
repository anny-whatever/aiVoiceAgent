{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "android": {
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET",
        "RECORD_AUDIO"
      ],
      "usesCleartextTraffic": false
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ]
  }
}
